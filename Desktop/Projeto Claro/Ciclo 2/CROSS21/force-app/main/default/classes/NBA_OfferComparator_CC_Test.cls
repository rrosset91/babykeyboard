	@IsTest
	private static void shouldInitOrderProperly() {
		Account conta = new Account();
		conta.Name = 'teste';
		conta.DocumentNumber__c = '123123123';
		insert conta;

		Contract contrato = new Contract();
		contrato.AccountId = conta.Id;
		contrato.CPF__c = '24312312';
        contrato.TV__c = true;
		contrato.CityCode__c = '13';
		insert contrato;

		Case c = new Case();
		
		c.RecordTypeId = Schema.SObjectType.Case.getRecordTypeInfosByDeveloperName().get('Claro_NBA_Retencao').getRecordTypeId();
		c.Status = 'New';
		c.ContractNumber__c = contrato.Id;
		c.Time_of_Service__c = 'Ofertas';
		c.CallReason__c = 'Falta de vantagens para Clientes';
		c.ProductTV__c = true;

		NBA_OfferComparator_CC.PosseCliente posse = new NBA_OfferComparator_CC.PosseCliente();
		posse.hasTv = true;
		posse.hasBroadBand = false;
		posse.hasPhone = true;
		posse.hasMovel = false;

		User atendente = [SELECT Id FROM User WHERE Profile.Name = 'Atendente NBA - Rentabilizacao Ativo' AND IsActive = true LIMIT 1];
		System.runAs(atendente) {
			insert c;
			NBA_OfferComparator_CC.initOrder(c.Id, JSON.serialize(posse));
		}

		c = [SELECT Order__c FROM Case WHERE Id = :c.Id];

		System.assertNotEquals(null, c.Order__c);
		Order pedido = [SELECT Id, OrderCase__c, TVMovement__c, VirtuaMovement__c, NETFoneMovement__c, MobileMovement__c FROM Order WHERE Id = :c.Order__c];

		System.assertNotEquals(null, pedido.OrderCase__c);
		System.assertEquals(null, pedido.TVMovement__c);
		System.assertEquals(NBA_Constants.MOVIMENTO_NENHUMA, pedido.VirtuaMovement__c);
		System.assertEquals(null, pedido.NETFoneMovement__c);
		System.assertEquals(NBA_Constants.MOVIMENTO_NENHUMA, pedido.MobileMovement__c);
	}
	
