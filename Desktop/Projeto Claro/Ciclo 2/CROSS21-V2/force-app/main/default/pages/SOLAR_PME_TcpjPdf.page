<!-- SOLAR_PME_TcpjPdf -->
<apex:page showHeader="false" applyHtmlTag="false" renderAs="Pdf" controller="SOLAR_PME_TcpjPdfController" >
    <head>
        <style> 
            @import url("https://fonts.googleapis.com/css2?family=Ropa+Sans");
            
            @page {
                @top-center {
                	content: element(header);
                }
                @bottom-left {
                	content: element(rubricbl);
                }
            	@bottom-center {
                	content: element(rubricbc);
                }
            	@bottom-right {
                	content: element(rubricbr);
                }
            	margin-top: 1.8cm;
            	margin-bottom: 2cm;
            }
            
            @page signature-page {
                @bottom-left {
                	content: none;
                }
            	@bottom-center {
                	content: none;
                }
            	@bottom-right {
                	content: none;
                }
            	height: 61rem;
            }
            
            .signature-page {
            	page: signature-page;
            }
            
            div.rubricbl {
            	position: running(rubricbl);
            }
            div.rubricbc {
            	position: running(rubricbc);
            }
            div.rubricbr {
            	position: running(rubricbr);
            }
            
            body {
            	font-family: 'Ropa Sans', sans-serif !important;
			}
            
            div.header {
            	text-align: center;
                padding: 5px;
                position: running(header);
                font: 14px bold;
                text-transform: uppercase;
            	font-family: 'Ropa Sans', sans-serif;
            }
            
            .header h3 {
				display: inline-block;
            	vertical-align: middle;
            }
            
            .header img {
            	display: inline-block;
            	vertical-align: middle;
            	margin-top: 5px;
            }
            
            #main {
            	font: 9px;
            	font-family: 'Ropa Sans', sans-serif;
            }
            
            p {
                padding: 1.5px;
                margin: 1px;
            	font-family: 'Ropa Sans', sans-serif;
            }
            
            h2 {
                background-color: #d5d5d5;
            	color: #303030;
                font-size: 14px;
                padding: 5px;
            	font-family: 'Ropa Sans', sans-serif;
           	   }
            
            .row {
            	display: block;
            }
            
            .colum {
            	vertical-align: top;
            	display: inline-block;
            }
            
            .options-group {
            	display: block;
            	width: 100%;
            	margin-bottom: 15px;
            	padding-left: 30px;
            }
            
            .options-group .colum {
            	vertical-align: middle;
            	height: 21px;
            }
            
            .w-100 {
            	width: 100%;
            }
            
            .w-95 {
            	/* for whitespaces */
            	width: 94%;
            }
            
            .w-90 {
            	width: 85%;
            }
            
            .w-85 {
            	width: 85%;
            }

            .w-70 {
            	width: 70%;
            }
            
            .w-50 {
            	width: 50%;
            }
            
            .w-40 {
            	width: 40%;
            }
            
            .w-30 {
            	width: 30%;
            }
            
            .w-25 {
            	width: 25%;
            }
            
            .w-20 {
            	width: 19%;
            }
            
            .w-15 {
            	width: 15%;
            }
            
            .w-14 {
            	width: 14%;
            }
            
            .w-12 {
            	width: 12%;
            }
            
            .w-10 {
            	width: 10%;
            }
            
            .w-8 {
            	width: 8%;
            }
            
            .w-5 {
            	width: 5%;
            }
            
            .subtitle {
            	font: 10px bold;
            	font-weight: bolder;
            	font-family: 'Ropa Sans', sans-serif;
            }
            
            .label {
            	font: 8px bold;
            	font-weight: bolder;
            	font-family: 'Ropa Sans', sans-serif;
            }
            
            .value {
            	font-size: 8px;
            }
            
            .value.center {
            	text-align: center;
            }
            
            .logo {
            	float: right;
            	height: 35px;
            }
            
            .square {
            	text-align: center;
                width: 20px;
                height: 20px;
                border: 2px solid;
                font-size: 12px;
            }
            
            .square p {
            	margin: -2px;
            }
            
            .anchors {
				color: #ffffff;
                font-size: 2px;
            	font-family: 'Ropa Sans', sans-serif;
            }

            .anchors.radio {	 
            	color: #ffffff;
                font-size: 1px;
            	opacity: 0;
            	margin-left: -30px;
            	padding-top: 14px;
            	font-family: 'Ropa Sans', sans-serif;
            }
            
            .ass-field {
            	height: 15px;
            	border-bottom: 2px solid;
            	margin-bottom: 0px;
            }
            
            .align-top-center {
            	vertical-align: top;
            	text-align: center;
            }
            
            .m-t-30 {
            	margin-top: 30px;
            }
            
            .m-t-60 {
            	margin-top: 60px;
            }
            
            .m-t-120 {
            	margin-top: 120px;
            }
            
            .mp-r-0 {
            	margin-right: 0px;
            	padding-right: 0px;
            }
            
            .float-right {
            	float: right;
            }
            
            .px {
            	font-size: 10px;
            	padding-top:5px;
            }
            
            .local-data {
            	margin: 0px;
            	padding: 0px;
            }
            
        </style>
    </head>
    
