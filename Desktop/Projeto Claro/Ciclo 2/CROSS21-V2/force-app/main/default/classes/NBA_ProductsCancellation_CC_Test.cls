    @IsTest 
    private static void shouldCreateOrderWithProperMovements() {
        Account conta = NBA_DataFactory_Test.getAccount();
		insert conta;
        
        Contract contrato = new Contract();
        contrato.AccountId = conta.Id;
        contrato.BL__c = true;
        insert contrato;
        
        Case caso = new Case();
        caso.Time_of_Service__c = 'Ofertas';
        caso.RecordTypeId = Schema.SObjectType.Case.getRecordTypeInfosByDeveloperName().get('Claro_NBA_Retencao').getRecordTypeId();
        caso.broadband__c = true;
        caso.AccountId = conta.Id;
        caso.ContractNumber__c = contrato.Id;
		caso.CallReason__c = '';

        NBA_OfferComparator_CC.Offer produtoAtual = new NBA_OfferComparator_CC.Offer();

        produtoAtual.tv.add(new NBA_OfferComparator_CC.PlanTv());
        produtoAtual.tv[0].planTv.label = NBA_Constants.MOVIMENTO_MANTER;
        
        produtoAtual.broadband.planBroadband.label = NBA_Constants.MOVIMENTO_MANTER;

        produtoAtual.phone.planPhone.label = NBA_Constants.NAO_POSSUI;

        produtoAtual.mobile.planMobile.label = NBA_Constants.NAO_POSSUI;

        String descriptionSchedule = '08:00:00 - 10:00:00';
        Date dateSchedule = Date.today();
        
        User atendente = [SELECT Id FROM User WHERE Profile.Name = 'Atendente NBA - Retencao' AND IsActive = true LIMIT 1];
		System.runAs(atendente) {
            insert caso;

            NBA_ProductsCancellation_CC.endCaseCancelation(caso.id, JSON.serialize(produtoAtual), dateSchedule, descriptionSchedule, false);
        }

        Order pedido = [SELECT Id, AccountId, ContractId, VirtuaMovement__c, TVMovement__c, NETFoneMovement__c, MobileMovement__c FROM Order WHERE OrderCase__c = :caso.Id];

        System.assertEquals(NBA_CONSTANTS.MOVIMENTO_CANCELAMENTO, pedido.TVMovement__c);
        System.assertEquals(NBA_CONSTANTS.MOVIMENTO_CANCELAMENTO, pedido.VirtuaMovement__c);
        System.assertEquals(NBA_Constants.MOVIMENTO_NENHUMA, pedido.NETFoneMovement__c);
        System.assertEquals(NBA_Constants.MOVIMENTO_NENHUMA, pedido.MobileMovement__c);
    }
    
