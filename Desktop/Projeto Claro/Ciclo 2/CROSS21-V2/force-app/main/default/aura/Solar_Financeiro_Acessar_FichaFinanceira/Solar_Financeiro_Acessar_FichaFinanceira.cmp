<aura:component implements="flexipage:availableForAllPageTypes,lightning:isUrlAddressable" access="global">
    <lightning:workspaceAPI aura:id="workspace" />
    <aura:attribute type="String" name="encodedbaseAttributes"/>
    <aura:attribute type="String" name="componentName"/>
    <aura:handler name="init" value="{!this}" action="{!c.init}"/>
    <div class="slds-tabs_card">
        <aura:renderIf isTrue="{!v.componentName == 'solarResFichaFinanceira'}">
            <c:solarResFichaFinanceira encodedbaseAttributes="{!v.encodedbaseAttributes}"/>
        </aura:renderIf>
        <aura:renderIf isTrue="{!v.componentName == 'solarFichaFinanceira'}">
            <c:solarFichaFinanceira encodedbaseAttributes="{!v.encodedbaseAttributes}"/>
        </aura:renderIf>
    </div>
</aura:component>